{
  "name": "community-trip-organizer",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node .",
    "dev": "yarn workspaces foreach -A -p -i run dev",
    "build": "yarn workspace backend run build && yarn workspace frontend run build",
    "lint": "yarn workspaces foreach -A -p -t run lint",
    "test": "yarn workspaces foreach -A -p -t run test",
    "format": "yarn workspaces foreach -A -p -t run format",
    "format:check": "prettier --check .",
    "db:migrate": "yarn workspace backend exec prisma migrate deploy",
    "db:seed": "yarn workspace backend run db:seed",
    "postinstall": "yarn workspace backend exec prisma generate"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "eslint": "9.33.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.2"
  },
  "packageManager": "yarn@4.10.2"
}
