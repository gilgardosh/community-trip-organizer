{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "dotenv -e .env.development -- node dist/src/index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "dev": "dotenv -e .env.development -- nodemon src/index.ts",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "server:dev": "yarn dev",
    "db:seed": "dotenv -e .env.development -- tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && tsx prisma/seed.ts",
    "db:dev:start": "docker-compose up -d",
    "db:dev:stop": "docker-compose down",
    "db:dev:restart": "docker-compose restart",
    "db:dev:logs": "docker-compose logs -f postgres",
    "db:dev:clean": "docker-compose down -v",
    "db:health": "dotenv -e .env.development -- ./scripts/db-health-check.sh",
    "db:test:start": "docker-compose -f docker-compose.test.yml up -d",
    "db:test:stop": "docker-compose -f docker-compose.test.yml down",
    "db:test:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "setup:dev": "docker-compose up -d && sleep 3 && npx prisma migrate dev && tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.14.0",
    "bcryptjs": "3.0.2",
    "cors": "2.8.5",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "express-async-handler": "1.2.0",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.1",
    "passport": "0.7.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.1",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/bcryptjs": "3.0.0",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/morgan": "1.9.10",
    "@types/node": "24.3.0",
    "@types/passport": "1.0.17",
    "@types/passport-facebook": "3.0.3",
    "@types/passport-google-oauth20": "2.0.16",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.3",
    "dotenv-cli": "10.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "execa": "9.6.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vitest": "3.2.4",
    "vitest-environment-prisma": "0.4.2"
  }
}
