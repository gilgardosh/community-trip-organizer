{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "dev": "nodemon src/index.ts",
    "test": "docker-compose -f docker-compose.test.yml up -d && yarn db:test:migrate && vitest",
    "db:test:start": "docker-compose -f docker-compose.test.yml up -d",
    "db:test:stop": "docker-compose -f docker-compose.test.yml down",
    "db:test:migrate": "dotenv -e .env.test -- prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.14.0",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/express": "5.0.3",
    "@types/node": "24.3.0",
    "dotenv-cli": "10.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "execa": "9.6.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vitest": "3.2.4",
    "vitest-environment-prisma": "0.4.2"
  }
}
